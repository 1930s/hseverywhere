{
  "name": "hseverywhere",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start-mobile": "PLATFORM=mobile node node_modules/react-native/local-cli/cli.js start --port=$PORT",
    "start-osx": "PLATFORM=osx node node_modules/react-native-desktop/local-cli/cli.js start --port=$PORT",
