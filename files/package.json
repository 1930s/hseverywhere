    "babel-preset-react": "6.5.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "react": "15.2.0",
    "react-dom": "15.2.0",
    "react-native-cli": "1.0.0",
    "react-native": "0.30.0",
    "react-native-web": "0.0.40",
    "react-native-desktop-cli": "0.1.11",
    "react-native-desktop": "0.9.0-rc1",
