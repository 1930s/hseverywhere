{
  "name": "hseverywhere",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start-mobile": "PLATFORM=mobile node node_modules/react-native/local-cli/cli.js start          --port=$PORT",
    "start-macos":  "PLATFORM=macos  node node_modules/react-native-macos/local-cli/cli.js start    --port=$PORT",
    "start-web":    "PLATFORM=web    node node_modules/webpack-dev-server/bin/webpack-dev-server.js --port=$PORT --progress --colors --content-base=../ --inline --hot",
    "pack": "node ./node_modules/webpack/bin/webpack.js"
  },
  "dependencies": {
    "rnpm": "1.9.0",
    "rnpm-plugin-windows": "0.1.1",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1",
    "json-loader": "0.5.4",
    "babel-loader": "6.2.4",
    "babel-core": "6.9.0",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "react": "15.4.1",
    "react-dom": "15.4.1",
    "react-native-cli": "2.0.1",
    "react-native": "0.42.3",
    "react-native-web": "0.0.88",
    "react-native-macos-cli": "0.2.1",
    "react-native-macos": "0.12.0-rc.2",
    "goog.crypt.Hash": "file:./goog.crypt.Hash",
