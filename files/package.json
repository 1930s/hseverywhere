{
  "name": "hseverywhere",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start-mobile": "PLATFORM=mobile node node_modules/react-native/local-cli/cli.js start          --port=$PORT",
    "start-macos":  "PLATFORM=macos  node node_modules/react-native-macos/local-cli/cli.js start    --port=$PORT",
    "start-web":    "PLATFORM=web    node node_modules/webpack-dev-server/bin/webpack-dev-server.js --port=$PORT --progress --colors --content-base=../ --inline --hot",
    "pack": "node ./node_modules/webpack/bin/webpack.js"
  },
  "babel": {
    "presets": ["react-native"],
    "ignore": [
      "all.js"
    ]
  },
  "dependencies": {
    "webpack": "4.13.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4",
    "json-loader": "0.5.4",
    "babel-loader": "7.1.4",
    "babel-core": "6.26.3",
    "babel-preset-env": "1.7.0",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-native": "4.0.0",
    "babel-plugin-add-module-exports": "0.2.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-native-cli": "2.0.1",
    "react-native": "0.51.0",
    "react-native-web": "0.8.5",
    "react-art": "16.0.0",
    "react-native-macos-cli": "2.0.1",
    "react-native-macos": "0.16.1",
    "react-native-webgl": "0.8.0",
    "create-react-class": "15.6.3",
    "prop-types": "15.6.2",
    "socket.io-client": "1.5.1",
    "Base64": "0.3.0"
  },
  "devDependencies": {
    "react-devtools": "3.2.3"
  }
}
